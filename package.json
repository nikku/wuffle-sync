{
  "name": "wuffle-sync",
  "version": "0.0.0",
  "description": "The wuffle sync bot",
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "main": "lib/index.js",
  "license": "MIT",
  "repository": "https://github.com/nikku/wuffle-sync.git",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {

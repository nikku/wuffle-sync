    "test": "mocha test",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "min-dash": "^3.4.0",
    "probot": "^7.2.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-bpmn-io": "^0.7.0",
    "mocha": "^6.0.2",
    "nock": "^10.0.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.5",
    "smee-client": "^1.0.2"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "mocha"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ],
    "ignore": [
      "tmp/*"
    ]
  }
